

<%= render 'errors' %>
<div class='form'>
  <form action="<%= users_url %>" method="post">
    <%= auth_token %>
    <input type="text" name="user[email]" value="<%= @user.email %>" placeholder="email">
    <input type="password" name="user[password]" value="<%= @user.password %>" placeholder="password">
    <input type="submit" value="Sign Up" class="submit">
  </form>
</div>
